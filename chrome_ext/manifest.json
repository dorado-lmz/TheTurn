{
"manifest_version": 2,
"name":"OTime",
"version": "1.0.0",
"description": "时间管理插件",
"icons":
  {
    "16": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  },
"permissions":[
    "http://*/",
    "https://*/"
    ],
"background": {
    "scripts": ["/js/vendor/jquery-3.3.1.min.js", "/js/storage.js","background.js"]
},
"web_accessible_resources":[
      "/css/content.css",
      "/js/page_content.js"
],
"browser_action": {
    "default_icon": "/images/logo.png",
    "default_title": "OTime",
    "default_popup": "popup.html"
  }
}



